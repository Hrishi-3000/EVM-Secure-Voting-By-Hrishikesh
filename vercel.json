{
  "version": 2,
  "builds": [
    {
      "src": "voting.web.py",
      "use": "@vercel/python"
    },
    {
      "src": "templates/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "voting.web.py"
    }
  ]
}
"routes": [
  ...,
  {
    "src": "/(.*\.(html|css|js))",
    "dest": "templates/$1"
  }
]
